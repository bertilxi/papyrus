{
  "tasks": {
    "start": "deno run -A main.ts",
    "dev": "WATCH=true deno run -A --watch=src,blocks dev.ts",
    "init": "deno run -A init.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
  },
  "importMap": "./importMap.json",
  "exclude": [".dist"],
  "lock": false
}
